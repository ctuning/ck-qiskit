{
  "backup_data_uid": "369940a853898e4f", 
  "data_name": "qiskit-vqe", 
  "no_compile": "yes", 
  "no_target_file": "yes", 
  "pass_env_to_resolve": "yes",
  "print_files_after_run": [
    "tmp-stdout.tmp", 
    "tmp-stderr.tmp"
  ], 
  "process_in_tmp": "yes", 
  "program": "yes", 
  "run_cmds": {
    "ibm-vqe-hydrogen": {
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file":"ibm_vqe_report.json",
        "output_invariant_of_repeat": "no",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../ibm_vqe_hydrogen.py --sample_number $<<VQE_SAMPLE_SIZE>>$ --q-device-name $<<VQE_QUANTUM_BACKEND>>$ --max_func_evaluations $<<VQE_MAX_ITERATIONS>>$ $<<VQE_VISUALIZE_ANSATZ>>$",
        "run_output_files": [
          "ibm_vqe_report.json"
        ]
      }
    }
  }, 
  "run_deps": {
    "IBM QX login": {
      "local": "yes",
      "name": "IBM QX credentials",
      "sort": 5,
      "disable_if_env": {
            "VQE_QUANTUM_BACKEND" : "local_qasm_simulator"
      },
      "tags": "ibmqx,login"
    },
    "python": {
      "local": "yes",
      "name": "Python 3",
      "sort": 10,
      "tags": "compiler,python",
      "version_from": [
            3,
            2,
            0
      ]
    },
    "lib-qiskit": {
      "local": "yes",
      "name": "IBM QuantumExperience PythonSDK",
      "skip_pruning_by_other_deps": "yes",
      "sort": 20,
      "tags": "lib,python-package,qiskit"
    },
    "optimizer-plugin": {
      "local": "yes",
      "name": "Deployed python optimizer function",
      "skip_pruning_by_other_deps": "yes",
      "sort": 30,
      "tags": "quantum,optimizer,deployed"
    },
    "vqe-utils-plugin": {
      "local": "yes",
      "name": "VQE Python utility functions",
      "skip_pruning_by_other_deps": "yes",
      "sort": 40,
      "tags": "quantum,vqe,utils,lib"
    },
    "ansatz-plugin": {
      "local": "yes",
      "name": "Qiskit ansatz function",
      "skip_pruning_by_other_deps": "yes",
      "sort": 50,
      "tags": "quantum,ansatz,deployed"
    }
  }, 
  "run_vars": {
    "VQE_SAMPLE_SIZE"           : 100,
    "VQE_MAX_ITERATIONS"        : 80,
    "VQE_QUANTUM_BACKEND"       : "local_qasm_simulator",
    "VQE_VISUALIZE_ANSATZ"      : ""
  }, 
  "tags": [
    "ibm",
    "ibmq",
    "ibmqx",
    "qiskit", 
    "qiskit-sdk-py",
    "demo",
    "examples",
    "vqe",
    "vqe-implementation",
    "hydrogen",
    "hackathon"
  ]
}
