{
  "auto_detect": "yes", 
  "customize": {
    "check_that_exists": "yes", 
    "env_prefix": "CK_ENV_LOGIN_IBMQX",
    "limit_recursion_dir_search": {
      "linux": 0,
      "win": 0
    }, 
    "detect_in_soft_dir": "yes",
    "soft_file_universal": ".cm$#sep#$meta.json",
    "env_mapping": [
        {
            "variable"      : "CK_IBM_API_TOKEN",
            "display_name"  : "your IBM QuantumExperience API key"
        },
        {
            "variable"      : "CK_IBM_API_EMAIL",
            "display_name"  : "your email that you used to register in IBM QuantumExperience"
        }
    ]
  }, 
  "soft_name": "IBM QuantumExperience Login", 
  "tags": [
    "dialogue",
    "ibm",
    "ibmq",
    "ibmqx",
    "login",
    "quantum",
    "qiskit",
    "api_key",
    "credentials",
    "password"
  ],
  "use_customize_script_from_another_entry": {
    "data_uoa": "f14e1ed51a944f13",
    "module_uoa": "soft"
  }
}
